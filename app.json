{
  "expo": {
    "name": "localServiceApp",
    "slug": "localServiceApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/service_icon_logo.png",
    "scheme": "localserviceapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "versionCode": 1,
      "package": "com.rishav_1.localServiceApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/service_icon_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c45554e4-0700-4164-bb17-fd6fb1930fc5"
      }
    },
    "owner": "rishav_1"
  }
}
